int shoporder()
{
	ofstream order("userorder.txt", ios::out);
	for (int i = 0; i < index; i++)
	{
		order << i<< "Your cloth type is " << obj3[i].clothtype1 << endl;
		order << i<< " Your cloth color is " << obj3[i].clothcolor1 << endl;
		order <<i<< "Your clothsize is " << obj3[i].clothsize1 << endl;
		order <<i<< " Your clothmaterial is " << obj3[i].clothmaterial1 << endl;
		order << i<< "Your total is " << obj3[i].total << endl;
	}
	order << "THE TOTAL SALE FOR YOUR SHOP FOR TODAY IS: " << sum;
	order.close();
	ifstream ad3("userorder.txt", ios::in);
	cout << "SHOP record:" << endl;
	while (ad3.eof() == 0) {
		string line;
		getline(ad3, line);
		cout << line << endl;
	}
	ad3.close();
	return 0;
}
