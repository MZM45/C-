	obj[6].clothtype = "tshirts";
	obj[0].price = 350;
	obj[1].price = 300;
	obj[2].price = 800;
	obj[3].price = 300;
	obj[4].price = 400;
	obj[5].price = 600;
	obj[6].price = 450;
	obj[0].clothmaterial = "COTTON";
	obj[1].clothmaterial = "SILK";
	obj[2].clothmaterial = "LINEN";
	obj[3].clothmaterial = "DOBBY";
	obj[4].clothmaterial = "JACQUARD";
	obj[5].clothmaterial = "BOSKI";
	obj[6].clothmaterial = "KHADDAR";
	obj[0].color = "RED";
	obj[1].color = "BLACK";
	obj[2].color = "BLUE";
	obj[3].color = "LIGHT BLUE";
	obj[4].color = "ORANGE";
	obj[5].color = "PURPLE";
	obj[6].color = "DARKBLUE";
	obj[0].size = "Small   ";
	obj[1].size = "XSmall  ";
	obj[2].size = "2XSmall ";
	obj[3].size = "Medium  ";
	obj[4].size = "Large   ";
	obj[5].size = "XLarge  ";
	obj[6].size = "2XLarge ";
	/////////////////////////////////////////////                                         ///////////////////////////////////////////
	////////////////////FILING///////////////////-----------------------------------------///////////////////FILING//////////////////-------------------------
	////////////////////////////////////////////                                          ///////////////////////////////////////////
	ofstream cloth("MALE CLOTH.txt", ios::out);
	for (int i = 0; i < 70; i++)
	{
		cloth << "-";
	}
	cloth << endl << "-------MALE CLOTHES ARE-------" << endl;
	for (int i = 0; i < 6; i++)
	{
		cloth << "|" << i << " : " << obj[i].clothtype << endl;
	}
	cloth << "-------MALE CLOTHES MATERIALS ARE-------" << endl;
	for (int i = 0; i < 6; i++)
	{
		cloth << "|" << i << " : " << obj[i].clothmaterial << endl;
	}
	cloth << "-------MALE CLOTHES COLOR ARE-------" << endl;
	for (int i = 0; i < 6; i++)
	{
		cloth << "|" << i << " : " << obj[i].color << "  " << endl;
	}
	for (int i = 0; i < 70; i++)
	{
		cloth << "-";
	}
	cloth.close();
	ifstream ad("MALE CLOTH.txt", ios::in);
	cout << "\t\t----------------------------" << endl;
	cout << "\t\t=======CLOTH RECORDS========" << endl;
	cout << "\t\t----------------------------"<<endl;
	while (ad.eof() == 0) {
		string line;
		getline(ad, line);
		cout << line << endl;
	}
	ad.close();
	return 0;
}
