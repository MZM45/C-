int female()
{
	system("cls");
	HANDLE h = GetStdHandle(STD_OUTPUT_HANDLE);
	SetConsoleTextAttribute(h, 11);
	cout << "\t\t\t--------------------------------------------------------------------------------------------" << endl;
	SetConsoleTextAttribute(h, 5);
	cout << "\t\t\t====================================FEMALE CLOTHES ENTRY===================================== " << endl;
	SetConsoleTextAttribute(h, 11);
	cout << "\t\t\t--------------------------------------------------------------------------------------------" << endl;
	cout << endl;
	char ch;
	do {
		char choice2;
		SetConsoleTextAttribute(h, 14);
		cout << "\t\t\t=========================================" << endl;
		SetConsoleTextAttribute(h, 11);
		cout << "\t\t\tPRESS 0 for PURCHASING ITEMS: " << endl;
		cout << "\t\t\tPRESS 1 FOR DISPLAY: " << endl;
		cout << "\t\t\tPRESS 2 FOR UPDATING YOUR PURCHASE: " << endl;
		SetConsoleTextAttribute(h, 14);
		cout << "\t\t\t=========================================";
		cin >> choice2;
		system("cls");
		if (choice2 == '1')
		{
			display();
		}
		if (choice2 == '2')
		{
			update();
		}
		if (choice2 == '0')
		{
			input2();
		}
		cout << "PRESS Y TO CONTINUE OR ANY KEY TO CHECKOUT and exit from cloth entries: " << endl;
		cin >> ch;
	} while (ch == 'Y' || ch == 'y');
	return 0;
}
