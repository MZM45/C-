	cout << endl;
	char ch;
	do {
		char choice2;
		SetConsoleTextAttribute(h, 14);
		cout << "\t\t\t========================================="<<endl;
		SetConsoleTextAttribute(h, 11);
		cout << "\t\t\tPRESS 0 for PURCHASING ITEMS: " << endl;
		cout << "\t\t\tPRESS 1 FOR DISPLAY: " << endl;
		cout << "\t\t\tPRESS 2 FOR UPDATING YOUR PURCHASE: " << endl;
		SetConsoleTextAttribute(h, 14);
		cout << "\t\t\t=========================================";
		cin >> choice2;
		system("cls");
		if (choice2 == '1')
		{
			display();
		}
		if (choice2 == '2')
		{
			update();
		}
		if (choice2 == '0')
		{
			input();
		}
		SetConsoleTextAttribute(h, 4);
		cout << "------------------PRESS Y TO CONTINUE OR ANY KEY TO CHECKOUT:------------------ " << endl;
		cin >> ch;
	} while (ch == 'Y' || ch == 'y');
	return 0;
}
int update()
{
	HANDLE h = GetStdHandle(STD_OUTPUT_HANDLE);
	int no, found = 0;
	SetConsoleTextAttribute(h, 5);
	cout << "\t\t============YOUR PURCHASES ARE=============" << endl;
	for (int i = 0; i < 90; i++)
	{
		SetConsoleTextAttribute(h, 14);
		cout << "-";
	}
	cout << endl;
	for (int j = 0; j < index; j++)
	{
		SetConsoleTextAttribute(h, 2);
		cout << "\t| NO OF PRODUCT" << j << ":" << "Your purchases are: " << endl;
		cout << "\t| NO OF PRODUCT" << j << ":" << " THE CLOTH TYPE IS: " << obj3[j].clothtype1 << endl;
		cout << "\t| NO OF PRODUCT" << j << ":" << " THE MATERIAL IS: " << obj3[j].clothmaterial1 << endl;
		cout << "\t| NO OF PRODUCT" << j << ":" << " THE COLOR IS: " << obj3[j].clothcolor1 << endl;
		cout << "\t| NO OF PRODUCT" << j << ":" << " The SIZE IS: " << obj3[j].clothsize1 << endl;
		cout << "\t| NO OF PRODUCT" << j << ":" << " The cost of PRODUCT purchased  is: " << obj3[j].total << endl;
		SetConsoleTextAttribute(h, 14);
		cout << "-------------------------------------------------------------------------------------------" << endl;
	}
	cout << endl;
	SetConsoleTextAttribute(h, 3);
	cout << "ENTER THE NUMBER OF PRODUCT YOU WANT TO REMOVE: ";
	cin >> no;
	for (int i = 0; i < 91; i++)
	{
		SetConsoleTextAttribute(h, 14);
		cout << "-";
	}
	cout << endl;
	for (int i = 0; i < index; i++)
	{
		if (no == obj3[i].id)
		{
			for (int k = i; k < index - 1; k++)
			{
				obj3[k].clothtype1 = obj3[k + 1].clothtype1;
				obj3[k].clothmaterial1 = obj3[k + 1].clothmaterial1;
				obj3[k].clothcolor1 = obj3[k + 1].clothcolor1;
				obj3[k].clothsize1 = obj3[k + 1].clothsize1;
				obj3[k + 1].total == 0;
			}
			cout << obj3[index].total << " is";
			index--;
			cout << " deleted\n";
			found++;
			break;
		}
	}
	sum = 0;
	for (int o = 0; o < index; o++)
	{
		sum += obj3[o].total;
	}
	if (found == 0)
	{
		SetConsoleTextAttribute(h, 4);
		cout << " \n * invalid **" << endl;
	}
	return sum;
}
