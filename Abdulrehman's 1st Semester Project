}
int admin()
{
	HANDLE h = GetStdHandle(STD_OUTPUT_HANDLE);
	/signup();/
	int a=0;
	char choiceadmin;
	do
	{
	   a==login();
	} while (a == 1 );
	char b;
	do

	{
		cout << endl;
		SetConsoleTextAttribute(h, 5);
		cout << "\t\t\t\t------------------------------------------------"<<endl;
		SetConsoleTextAttribute(h, 11);
		cout << "\t\t\t\t Press 1 to view male cloth items: " << endl;
		cout << "\t\t\t\t Press 2 to view female cloth items: " << endl;
		cout << "\t\t\t\t Press 3 to view shop bill: " << endl;
		SetConsoleTextAttribute(h, 5);
		cout << "\t\t\t\t------------------------------------------------"<<endl;
		cin >> choiceadmin;
		if (choiceadmin == '1')
		{
			SetConsoleTextAttribute(h, 11);
			male2();
		}
		if (choiceadmin == '2')
		{
			SetConsoleTextAttribute(h, 11);
			female2();
		}
		if (choiceadmin == '3')
		{
			SetConsoleTextAttribute(h, 2);
			shoporder();
		}
		SetConsoleTextAttribute(h, 4);
		cout << "press y to continue or any key to exit: " << endl;
		cin >> b;
		system("cls");
	} while (b == 'y' || b == 'Y');
	return 0;
}
int male()
{
